<!-- latest code for photo search -->
<%= render :partial => "artists/photo_menu", :locals => {:artist => @artist} %>
<section id="content">
    <div id="shop-container" class="container transitions-enabled clearfix" style="margin: 0 auto;">
        <ul class="artists_items" style="margin: 0 auto;">
            <% @images.reverse.each do |image| %>
                <li class="shop-box" style="height: <%= image.medium_height %>px;" data-id = "<%= image.id %>">
                    <%= link_to image_tag(asset_path("lazy_img_load.gif"),
                    :class => 'lazy',
                    :style => "height: #{image.thumb_height}px;",
                    :data => {"original" => image.image.url(:thumb).to_s}), 'javascript:void(0);' %>
                </li> 
            <% end %>
        </ul>
    </div>
</section>

<script>!function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0],p=/^http:/.test(d.location)?'http':'https';if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src=p+'://platform.twitter.com/widgets.js';fjs.parentNode.insertBefore(js,fjs);}}(document, 'script', 'twitter-wjs');</script>
